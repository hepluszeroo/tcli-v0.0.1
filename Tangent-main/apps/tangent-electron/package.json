{
  "name": "tangent_electron",
  "productName": "Tangent",
  "version": "0.9.0-alpha.10",
  "description": "Your Notes, Your Thoughts; Your Tangent",
  "repository": "https://github.com/suchnsuch/Tangent",
  "license": "Apache-2.0",
  "main": "__build/bundle/main.js",
  "scripts": {
    "electron": "cross-env NODE_ENV=development electron ./__build",
    "dev": "NODE_ENV=development node dev.js",
    "dev:debug": "DEBUG=5858 NODE_ENV=development node dev.js",
    "dev:prod": "node dev.js",
    "dev:win": "SET \"NODE_ENV=development\" && node dev.js",
    "build": "node build/index.js",
    "build:dev": "NODE_ENV=development NO_WATCH=true npm run build",
    "build:test-e2e": "rimraf __build && cross-env NODE_ENV=development E2E_TEST=1 VITE_E2E=1 node build/build-e2e.js",
    "pretest:e2e": "pnpm run build:test-e2e",
    "clean": "rm -rf dist ; rm -rf __build",
    "clean-build": "npm run clean && npm run build",
    "release": "electron-builder",
    "package:test": "electron-builder --publish never --dir -c.mac.identity=null",
    "release:test-notarize": "NOTARIZE=force electron-builder --publish never",
    "publish:mac": "electron-builder --mac --publish always",
    "package:mac": "electron-builder --mac --publish never",
    "publish:win": "electron-builder --win --publish always",
    "package:win": "electron-builder --win --publish never",
    "publish:linux": "electron-builder --linux --publish always",
    "package:linux": "electron-builder --linux --publish never",
    "publish:all": "electron-builder -mwl --publish always",
    "package:all": "electron-builder -mwl --publish never",
    "build-and-release:all": "npm run clean-build && npm run test:no-build && npm run publish:all",
    "test": "vitest run && npm run build:dev && npm run test:end-to-end",
    "test:no-build": "vitest run && npm run test:end-to-end",
    "test:unit": "vitest run",
    "test:integration": "vitest --project integration",
    "test:end-to-end": "playwright test --project Tests",
    "test:e2e": "npm run build:test-e2e && cross-env ELECTRON_ENABLE_LOGGING=1 playwright test tests-integration",
    "svelte-dev": "webpack watch"
  },
  "keywords": [],
  "author": {
    "name": "Taylor Hadden",
    "email": "taylorhadden@icloud.com"
  },
  "dependencies": {
    "@typewriter/delta": "^1.0.2",
    "@typewriter/document": "^0.9.4",
    "adm-zip": "^0.5.9",
    "chalk": "^4.1.1",
    "cheerio": "^1.0.0-rc.10",
    "dotenv": "^9.0.2",
    "electron-reload": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.1.4",
    "fast-equals": "^2.0.4",
    "font-list": "^1.4.0",
    "js-logger": "^1.6.1",
    "link-preview-js": "^3.0.12",
    "node-fetch": "^3.3.0",
    "prismjs": "^1.24.1",
    "semver": "^7.3.7",
    "source-map-support": "^0.5.19",
    "svelte": "^4.2.3",
    "typewriter-editor": "file:../../lib/typewriter",
    "vscode-oniguruma": "^1.6.2",
    "vscode-textmate": "^7.0.1",
    "watcher": "^1.2.0",
    "yaml": "^2.0.0-10",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@electron/notarize": "^1.2.3",
    "@floating-ui/dom": "^1.6.13",
    "@playwright/test": "1.52.0",
    "@popperjs/core": "^2.9.2",
    "@such-n-such/core": "*",
    "@such-n-such/tangent-html-to-markdown": "*",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tsconfig/svelte": "^5.0.0",
    "@types/node": "^20.17.2",
    "@types/prismjs": "^1.16.6",
    "@vitest/browser": "^2.1.8",
    "adm-zip": "^0.5.9",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "electron": "35.2.1",
    "electron-builder": "^24.13.3",
    "happy-dom": "^15.11.7",
    "katex": "^0.16.8",
    "language-name-map": "^0.3.0",
    "lorem-ipsum": "^2.0.8",
    "mini-css-extract-plugin": "^1.3.4",
    "playwright": "^1.52.0",
    "prism-svelte": "^0.4.7",
    "quill-delta": "4.2.2",
    "sass": "^1.32.10",
    "sass-loader": "^11.0.1",
    "svelte-dnd-action": "^0.9.14",
    "svelte-loader": "^3.1.8",
    "svelte-preprocess": "^5.0.4",
    "ts-loader": "^9.4.4",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^5.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0"
    ,
  },
  "optionalDependencies": {},
  "pnpm": {
    "overrides": {
      "quill-delta": "4.2.2"
    }
  }
}
